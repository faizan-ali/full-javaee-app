        <class>co.workamerica.entities.clients.ProfileViews</class>
        <class>co.workamerica.entities.criteria.Certifications</class>
        <class>co.workamerica.entities.criteria.Fields</class>
        <class>co.workamerica.entities.criteria.Schools</class>
        <class>co.workamerica.entities.criteria.States</class>
        <class>co.workamerica.entities.logs.clients.ClientLoginLogs</class>
        <class>co.workamerica.entities.logs.clients.PipelineChangeLogs</class>
        <class>co.workamerica.entities.logs.clients.ProfileViewLogs</class>
        <class>co.workamerica.entities.logs.clients.SearchLogs</class>
        <class>co.workamerica.entities.logs.candidates.CandidateLoginLogs</class>
